# AI Learning Insight API - Environment Variables Template
# Copy file ini menjadi .env dan isi dengan nilai yang sesuai

# Gemini AI API Key untuk Model 2 (Advice Generation)
# Dapatkan dari: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Database Configuration (Untuk Tim Backend)
# Uncomment dan isi jika API ini akan connect langsung ke database
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=learning_db
# DB_USER=your_db_user
# DB_PASSWORD=your_db_password

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_RELOAD=True

# CORS Origins (comma separated)
# Dalam production, ganti dengan domain frontend yang spesifik
CORS_ORIGINS=http://localhost:3000,http://localhost:5173
